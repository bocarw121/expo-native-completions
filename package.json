{
  "name": "expo-native-completions",
  "displayName": "Expo Native Completions",
  "description": "Provides code completions for Expo projects in Kotlin and Swift.",
  "version": "0.0.1",
  "publisher": "Bocar",
  "engines": {
    "vscode": "^1.90.0"
  },
  "icon": "image/icon.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/bocarw121/expo-native-completions.git"
  },
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/app.json",
    "workspaceContains:**/app.config.js",
    "workspaceContains:**/app.config.ts"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "publish": "vsce publish",
    "prepublish": "pnpm run test",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.9",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  }
}
